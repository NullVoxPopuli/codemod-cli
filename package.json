{
  "name": "codemod-cli",
  "version": "0.2.11",
  "description": "a command line tool for generating, testing, and publishing codemods",
  "keywords": [
    "cli",
    "codemod"
  ],
  "homepage": "https://github.com/rwjblue/codemod-cli#readme",
  "bugs": {
    "url": "https://github.com/rwjblue/codemod-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rwjblue/codemod-cli.git"
  },
  "license": "MIT",
  "author": "Robert Jackson <me@rwjblue.com>",
  "files": [
    "src",
    "bin",
    "commands"
  ],
  "main": "src/index.js",
  "bin": {
    "codemod-cli": "./bin/cli.js"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "lint:js": "eslint .",
    "release": "release-it",
    "test": "qunit tests/**/*-test.js"
  },
  "dependencies": {
    "@babel/parser": "^7.3.2",
    "chalk": "^2.4.2",
    "common-tags": "^1.8.0",
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "globby": "^9.0.0",
    "import-cwd": "^2.1.0",
    "import-local": "^2.0.0",
    "jscodeshift": "^0.6.3",
    "pkg-up": "^2.0.0",
    "recast": "^0.17.3",
    "yargs": "^13.2.0"
  },
  "devDependencies": {
    "broccoli-test-helper": "^2.0.0",
    "co": "^4.6.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.14.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "qunit": "^2.9.1",
    "release-it": "^12.2.1",
    "release-it-lerna-changelog": "^1.0.3",
    "require-so-slow": "^1.2.0",
    "walk-sync": "^1.1.3"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  }
}
